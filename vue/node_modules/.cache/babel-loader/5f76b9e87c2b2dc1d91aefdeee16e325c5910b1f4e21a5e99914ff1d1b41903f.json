{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport * as echarts from \"echarts\";\nimport WelcomeComponent from \"@/components/Welcome.vue\";\nlet pieOptions = {\n  title: {\n    text: '',\n    // 主标题\n    subtext: '',\n    // 副标题\n    left: 'center'\n  },\n  tooltip: {\n    trigger: 'item',\n    formatter: '{a} <br/>{b} : {c} ({d}%)'\n  },\n  legend: {\n    orient: 'vertical',\n    left: 'left'\n  },\n  series: [{\n    name: '',\n    // 鼠标移上去显示内容\n    type: 'pie',\n    radius: '50%',\n    center: ['50%', '60%'],\n    data: [{\n      value: 1048,\n      name: '瑞幸咖啡'\n    },\n    // 示例数据：name表示维度，value表示对应的值\n    {\n      value: 735,\n      name: '雀巢咖啡'\n    }, {\n      value: 580,\n      name: '星巴克咖啡'\n    }, {\n      value: 484,\n      name: '栖巢咖啡'\n    }, {\n      value: 300,\n      name: '小武哥咖啡'\n    }]\n  }]\n};\nlet lineOptions = {\n  title: {\n    text: '',\n    // 主标题\n    subtext: '',\n    // 副标题\n    left: 'center'\n  },\n  xAxis: {\n    type: 'category',\n    data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] // 示例数据：统计的维度（横坐标）\n  },\n  yAxis: {\n    type: 'value'\n  },\n  tooltip: {\n    trigger: 'item'\n  },\n  series: [{\n    data: [120, 200, 150, 80, 70, 110, 130],\n    // 示例数据：横坐标维度对应的值（纵坐标）\n    type: 'line'\n  }]\n};\nlet barOptions = {\n  title: {\n    text: '',\n    // 主标题\n    subtext: '',\n    // 副标题\n    left: 'center'\n  },\n  xAxis: {\n    type: 'category',\n    data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] // 示例数据：统计的维度（横坐标）\n  },\n  yAxis: {\n    type: 'value'\n  },\n  tooltip: {\n    trigger: 'item'\n  },\n  series: [{\n    data: [120, 200, 150, 80, 70, 110, 130],\n    // 示例数据：横坐标维度对应的值（纵坐标）\n    type: 'bar',\n    itemStyle: {\n      normal: {\n        color: function () {\n          return \"#\" + Math.floor(Math.random() * (256 * 256 * 256 - 1)).toString(16);\n        }\n      }\n    }\n  }]\n};\nlet barSalaryOptions = {\n  title: {\n    text: '',\n    // 主标题\n    subtext: '',\n    // 副标题\n    left: 'center'\n  },\n  xAxis: {\n    type: 'category',\n    data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] // 示例数据：统计的维度（横坐标）\n  },\n  yAxis: {\n    type: 'value'\n  },\n  tooltip: {\n    trigger: 'item'\n  },\n  series: [{\n    data: [120, 200, 150, 80, 70, 110, 130],\n    // 示例数据：横坐标维度对应的值（纵坐标）\n    type: 'bar',\n    itemStyle: {\n      normal: {\n        color: function () {\n          return \"#\" + Math.floor(Math.random() * (256 * 256 * 256 - 1)).toString(16);\n        }\n      }\n    }\n  }]\n};\nexport default {\n  name: 'Home',\n  components: {\n    WelcomeComponent\n  },\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      notices: [],\n      month: null,\n      monthData: [],\n      departmentInfo: {\n        name: '',\n        employeeCount: 0,\n        headerName: ''\n      },\n      pendingTasks: {\n        askCount: 0,\n        resourceCount: 0\n      },\n      personalTasks: {\n        total: 0,\n        approved: 0,\n        pending: 0,\n        rejected: 0\n      },\n      departmentEmployees: [],\n      mockData: {\n        departments: {\n          1: {\n            name: '技术研发部',\n            headerName: '刘波涛'\n          },\n          2: {\n            name: '市场营销部',\n            headerName: '张静'\n          },\n          3: {\n            name: '人力资源部',\n            headerName: '王飞'\n          },\n          4: {\n            name: '财务部',\n            headerName: '刘颖'\n          },\n          5: {\n            name: '运营部',\n            headerName: '陈伟'\n          }\n        },\n        employeeCounts: {\n          1: 3,\n          2: 2,\n          3: 2,\n          4: 1,\n          5: 1\n        },\n        pendingAsks: {\n          1: 2,\n          2: 1,\n          3: 1,\n          4: 0,\n          5: 1\n        },\n        pendingResources: {\n          1: 2,\n          2: 1,\n          3: 0,\n          4: 1,\n          5: 0\n        },\n        departmentEmployees: {\n          1: [{\n            id: 1,\n            name: '张三',\n            phone: '13811111111',\n            email: 'zhangsan@xm.com',\n            level: '员工'\n          }, {\n            id: 2,\n            name: '李四',\n            phone: '13822222222',\n            email: 'lisi@xm.com',\n            level: '员工'\n          }, {\n            id: 4,\n            name: '刘波涛',\n            phone: '13844444444',\n            email: 'liubotao@xm.com',\n            level: '主管'\n          }],\n          2: [{\n            id: 3,\n            name: '王五',\n            phone: '13833333333',\n            email: 'wangwu@xm.com',\n            level: '员工'\n          }, {\n            id: 5,\n            name: '张静',\n            phone: '13855555555',\n            email: 'zhangjing@xm.com',\n            level: '主管'\n          }],\n          3: [{\n            id: 6,\n            name: '王飞',\n            phone: '13866666666',\n            email: 'wangfei@xm.com',\n            level: '主管'\n          }, {\n            id: 10,\n            name: '孙莹',\n            phone: '13800000000',\n            email: 'sunying@xm.com',\n            level: '员工'\n          }],\n          4: [{\n            id: 7,\n            name: '刘颖',\n            phone: '13877777777',\n            email: 'liuying@xm.com',\n            level: '主管'\n          }],\n          5: [{\n            id: 8,\n            name: '陈伟',\n            phone: '13888888888',\n            email: 'chenwei@xm.com',\n            level: '主管'\n          }]\n        },\n        personalStats: {\n          total: 3,\n          approved: 1,\n          pending: 1,\n          rejected: 1\n        }\n      }\n    };\n  },\n  computed: {\n    isHeader() {\n      return this.user.level === 'HEADER' || this.user.role === 'HEADER' || this.user.level && this.user.level.includes('主管');\n    }\n  },\n  created() {\n    this.$request.get('/notice/selectAll').then(res => {\n      this.notices = res.data || [];\n    });\n    if (this.user.role === 'ADMIN') {\n      this.getMonthData();\n      this.getFinancialPie();\n      this.getFinancialLine();\n      this.getFinancialBar();\n    } else if (this.isHeader) {\n      this.loadDepartmentInfo();\n      this.loadPendingTasks();\n      this.loadDepartmentEmployees();\n    } else if (this.user.role === 'USER') {\n      this.loadDepartmentInfo();\n      this.loadPersonalTasks();\n    }\n  },\n  methods: {\n    getMonthData() {\n      this.$request.get('/salary/getMonth').then(res => {\n        if (res.code === '200') {\n          this.monthData = res.data;\n          this.month = this.monthData[0].year;\n          this.getSalaryMonth(this.month);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    getSalaryMonth(month) {\n      this.$request.get('/salary/getSalary/' + month).then(res => {\n        if (res.code === '200') {\n          let chartDom = document.getElementById('salaryBar');\n          let myChart = echarts.init(chartDom);\n          barSalaryOptions.title.text = res.data.text;\n          barSalaryOptions.title.subtext = res.data.subtext;\n          barSalaryOptions.xAxis.data = res.data.xAxis;\n          barSalaryOptions.series[0].data = res.data.yAxis;\n          myChart.setOption(barSalaryOptions);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    getFinancialPie() {\n      this.$request.get('/financial/getPie').then(res => {\n        if (res.code === '200') {\n          let chartDom = document.getElementById('pie');\n          let myChart = echarts.init(chartDom);\n          pieOptions.title.text = res.data.text;\n          pieOptions.title.subtext = res.data.subtext;\n          pieOptions.series[0].name = res.data.name;\n          pieOptions.series[0].data = res.data.data;\n          myChart.setOption(pieOptions);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    getFinancialLine() {\n      this.$request.get('/financial/getLine').then(res => {\n        if (res.code === '200') {\n          let chartDom = document.getElementById('line');\n          let myChart = echarts.init(chartDom);\n          lineOptions.title.text = res.data.text;\n          lineOptions.title.subtext = res.data.subtext;\n          lineOptions.xAxis.data = res.data.xAxis;\n          lineOptions.series[0].data = res.data.yAxis;\n          myChart.setOption(lineOptions);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    getFinancialBar() {\n      this.$request.get('/financial/getBar').then(res => {\n        if (res.code === '200') {\n          let chartDom = document.getElementById('bar');\n          let myChart = echarts.init(chartDom);\n          barOptions.title.text = res.data.text;\n          barOptions.title.subtext = res.data.subtext;\n          barOptions.xAxis.data = res.data.xAxis;\n          barOptions.series[0].data = res.data.yAxis;\n          myChart.setOption(barOptions);\n        }\n      });\n    },\n    loadDepartmentInfo() {\n      if (this.user.departmentId) {\n        this.$request.get('/department/findById/' + this.user.departmentId).then(res => {\n          if (res.code === '200') {\n            this.departmentInfo.name = res.data.name;\n            this.departmentInfo.headerName = res.data.headerName || '未指定';\n            if (this.isHeader) {\n              this.loadEmployeeCount();\n            }\n          }\n        }).catch(error => {\n          console.error(\"获取部门信息失败:\", error);\n          const deptId = this.user.departmentId;\n          if (this.mockData.departments[deptId]) {\n            console.log(\"使用模拟部门数据\");\n            this.departmentInfo.name = this.mockData.departments[deptId].name;\n            this.departmentInfo.headerName = this.mockData.departments[deptId].headerName;\n            if (this.isHeader) {\n              this.loadEmployeeCount();\n            }\n          } else {\n            this.$message.error(\"获取部门信息失败，请检查网络连接\");\n          }\n        });\n      }\n    },\n    loadEmployeeCount() {\n      this.$request.get('/employee/count', {\n        params: {\n          departmentId: this.user.departmentId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.departmentInfo.employeeCount = res.data || 0;\n        }\n      }).catch(error => {\n        console.error(\"获取员工数量失败:\", error);\n        const deptId = this.user.departmentId;\n        if (this.mockData.employeeCounts[deptId] !== undefined) {\n          console.log(\"使用模拟员工数量数据\");\n          this.departmentInfo.employeeCount = this.mockData.employeeCounts[deptId];\n        } else {\n          this.$message.error(\"获取员工数量失败，请检查网络连接\");\n        }\n      });\n    },\n    loadPendingTasks() {\n      this.loadPendingAskTasks();\n      this.loadPendingResourceTasks();\n    },\n    loadPendingAskTasks() {\n      this.$request.get('/askapply/countPending', {\n        params: {\n          departmentId: this.user.departmentId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.pendingTasks.askCount = res.data || 0;\n        }\n      }).catch(error => {\n        console.error(\"获取待审批请假数据失败:\", error);\n        const deptId = this.user.departmentId;\n        if (this.mockData.pendingAsks[deptId] !== undefined) {\n          console.log(\"使用模拟待审批请假数据\");\n          this.pendingTasks.askCount = this.mockData.pendingAsks[deptId];\n        } else {\n          this.$message.error(\"获取待审批请假数据失败\");\n        }\n      });\n    },\n    loadPendingResourceTasks() {\n      this.$request.get('/resourceapply/countPending', {\n        params: {\n          departmentId: this.user.departmentId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.pendingTasks.resourceCount = res.data || 0;\n        }\n      }).catch(error => {\n        console.error(\"获取待审批资产数据失败:\", error);\n        const deptId = this.user.departmentId;\n        if (this.mockData.pendingResources[deptId] !== undefined) {\n          console.log(\"使用模拟待审批资产数据\");\n          this.pendingTasks.resourceCount = this.mockData.pendingResources[deptId];\n        } else {\n          this.$message.error(\"获取待审批资产数据失败\");\n        }\n      });\n    },\n    loadDepartmentEmployees() {\n      this.$request.get('/employee/selectByDepartment', {\n        params: {\n          departmentId: this.user.departmentId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.departmentEmployees = res.data || [];\n        }\n      }).catch(error => {\n        console.error(\"获取部门员工列表失败:\", error);\n        const deptId = this.user.departmentId;\n        if (this.mockData.departmentEmployees[deptId]) {\n          console.log(\"使用模拟部门员工列表数据\");\n          this.departmentEmployees = this.mockData.departmentEmployees[deptId];\n        } else {\n          this.$message.error(\"获取部门员工列表失败\");\n        }\n      });\n    },\n    loadPersonalTasks() {\n      this.$request.get('/employee/applicationStats', {\n        params: {\n          employeeId: this.user.id\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.personalTasks = res.data || {\n            total: 0,\n            approved: 0,\n            pending: 0,\n            rejected: 0\n          };\n        }\n      }).catch(error => {\n        console.error(\"获取个人申请统计数据失败:\", error);\n        console.log(\"使用模拟个人申请统计数据\");\n        this.personalTasks = this.mockData.personalStats;\n      });\n    },\n    goToEmployeeList() {\n      this.$router.push('/user');\n    }\n  }\n};","map":{"version":3,"names":["echarts","WelcomeComponent","pieOptions","title","text","subtext","left","tooltip","trigger","formatter","legend","orient","series","name","type","radius","center","data","value","lineOptions","xAxis","yAxis","barOptions","itemStyle","normal","color","Math","floor","random","toString","barSalaryOptions","components","user","JSON","parse","localStorage","getItem","notices","month","monthData","departmentInfo","employeeCount","headerName","pendingTasks","askCount","resourceCount","personalTasks","total","approved","pending","rejected","departmentEmployees","mockData","departments","employeeCounts","pendingAsks","pendingResources","id","phone","email","level","personalStats","computed","isHeader","role","includes","created","$request","get","then","res","getMonthData","getFinancialPie","getFinancialLine","getFinancialBar","loadDepartmentInfo","loadPendingTasks","loadDepartmentEmployees","loadPersonalTasks","methods","code","year","getSalaryMonth","$message","error","msg","chartDom","document","getElementById","myChart","init","setOption","departmentId","loadEmployeeCount","catch","console","deptId","log","params","undefined","loadPendingAskTasks","loadPendingResourceTasks","employeeId","goToEmployeeList","$router","push"],"sources":["src/views/manager/Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 主管仪表盘 -->\r\n    <div v-if=\"isHeader\" class=\"dashboard-header\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-card shadow=\"hover\">\r\n            <div slot=\"header\">\r\n              <span><i class=\"el-icon-s-custom\"></i> 部门管理</span>\r\n            </div>\r\n            <div class=\"dashboard-item\">\r\n              <div class=\"dashboard-num\">{{ departmentInfo.name || '暂无数据' }}</div>\r\n              <div class=\"dashboard-title\">当前管理部门</div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-card shadow=\"hover\">\r\n            <div slot=\"header\">\r\n              <span><i class=\"el-icon-user\"></i> 员工管理</span>\r\n            </div>\r\n            <div class=\"dashboard-item\">\r\n              <div class=\"dashboard-num\">{{ departmentInfo.employeeCount || 0 }}</div>\r\n              <div class=\"dashboard-title\">部门员工数</div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-card shadow=\"hover\">\r\n            <div slot=\"header\">\r\n              <span><i class=\"el-icon-s-claim\"></i> 待办事项</span>\r\n            </div>\r\n            <div class=\"dashboard-item\">\r\n              <div class=\"dashboard-num\">{{ pendingTasks.askCount + pendingTasks.resourceCount }}</div>\r\n              <div class=\"dashboard-title\">待审批事项</div>\r\n              <div class=\"dashboard-detail\">\r\n                <el-tag size=\"small\" type=\"warning\">请假审批: {{ pendingTasks.askCount }}</el-tag>\r\n                <el-tag size=\"small\" type=\"warning\" style=\"margin-left: 10px\">资产审批: {{ pendingTasks.resourceCount }}</el-tag>\r\n              </div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <div style=\"margin-top: 20px\">\r\n        <el-card shadow=\"hover\">\r\n          <div slot=\"header\">\r\n            <span><i class=\"el-icon-s-grid\"></i> 部门员工</span>\r\n            <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"goToEmployeeList\">查看全部</el-button>\r\n          </div>\r\n          <el-table :data=\"departmentEmployees\" stripe style=\"width: 100%\">\r\n            <el-table-column prop=\"name\" label=\"姓名\"></el-table-column>\r\n            <el-table-column prop=\"phone\" label=\"联系电话\"></el-table-column>\r\n            <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n            <el-table-column prop=\"level\" label=\"级别\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag size=\"mini\" v-if=\"scope.row.level === 'HEADER'\" type=\"success\">部门主管</el-tag>\r\n                <el-tag size=\"mini\" v-else-if=\"scope.row.level === 'EMPLOYEE'\" type=\"info\">普通员工</el-tag>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 员工仪表盘 -->\r\n    <div v-else-if=\"user.role === 'USER' && !isHeader\" class=\"dashboard-employee\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-card shadow=\"hover\">\r\n            <div slot=\"header\">\r\n              <span><i class=\"el-icon-office-building\"></i> 所属部门</span>\r\n            </div>\r\n            <div class=\"dashboard-item\">\r\n              <div class=\"dashboard-num\">{{ user.departmentName || '暂无数据' }}</div>\r\n              <div class=\"dashboard-title\">当前部门</div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-card shadow=\"hover\">\r\n            <div slot=\"header\">\r\n              <span><i class=\"el-icon-user\"></i> 直属主管</span>\r\n            </div>\r\n            <div class=\"dashboard-item\">\r\n              <div class=\"dashboard-num\">{{ departmentInfo.headerName || '未指定' }}</div>\r\n              <div class=\"dashboard-title\">部门主管</div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-card shadow=\"hover\">\r\n            <div slot=\"header\">\r\n              <span><i class=\"el-icon-s-claim\"></i> 个人申请</span>\r\n            </div>\r\n            <div class=\"dashboard-item\">\r\n              <div class=\"dashboard-num\">{{ personalTasks.total || 0 }}</div>\r\n              <div class=\"dashboard-title\">申请事项</div>\r\n              <div class=\"dashboard-detail\">\r\n                <el-tag size=\"small\" type=\"success\">已通过: {{ personalTasks.approved }}</el-tag>\r\n                <el-tag size=\"small\" type=\"warning\" style=\"margin-left: 10px\">审批中: {{ personalTasks.pending }}</el-tag>\r\n                <el-tag size=\"small\" type=\"danger\" style=\"margin-left: 10px\">已拒绝: {{ personalTasks.rejected }}</el-tag>\r\n              </div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n      </el-row>\r\n      \r\n      <!-- 添加欢迎组件 -->\r\n      <el-card style=\"margin-top: 20px\">\r\n        <welcome-component></welcome-component>\r\n      </el-card>\r\n    </div>\r\n\r\n    <!-- 管理员仪表盘 -->\r\n    <div v-else>\r\n      <div class=\"card\" style=\"padding: 15px\">\r\n        您好，{{ user?.name }}！欢迎使用本系统\r\n      </div>\r\n\r\n      <div style=\"display: flex; margin: 10px 0\">\r\n        <div style=\"width: 100%;\" class=\"card\">\r\n          <div style=\"margin-bottom: 30px; font-size: 20px; font-weight: bold\">公告列表</div>\r\n          <div>\r\n            <el-timeline  reverse slot=\"reference\">\r\n              <el-timeline-item v-for=\"item in notices\" :key=\"item.id\" :timestamp=\"item.time\">\r\n                <el-popover\r\n                    placement=\"right\"\r\n                    width=\"200\"\r\n                    trigger=\"hover\"\r\n                    :content=\"item.content\">\r\n                  <span slot=\"reference\">{{ item.title }}</span>\r\n                </el-popover>\r\n              </el-timeline-item>\r\n            </el-timeline>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div style=\"display: flex\" v-if=\"user.role === 'ADMIN'\">\r\n        <div id=\"pie\" style=\"height: 450px; flex: 1\" class=\"card\"></div>\r\n        <div style=\"height: 450px; flex: 1;\" class=\"card\">\r\n          <el-select v-model=\"month\" style=\"width: 50%\" @change=\"getSalaryMonth(month)\">\r\n            <el-option v-for=\"item in monthData\" :label=\"item.year\" :value=\"item.year\"></el-option>\r\n          </el-select>\r\n          <div id=\"salaryBar\" style=\"height: 400px; padding-top: 15px\"></div>\r\n        </div>\r\n      </div>\r\n      <div style=\"margin-top: 10px; display: flex\" v-if=\"user.role === 'ADMIN'\">\r\n        <div id=\"line\" style=\"height: 400px; flex: 1\" class=\"card\"></div>\r\n        <div id=\"bar\" style=\"height: 400px; flex: 1\" class=\"card\"></div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from \"echarts\";\r\nimport WelcomeComponent from \"@/components/Welcome.vue\";\r\n\r\nlet pieOptions = {\r\n  title: {\r\n    text: '', // 主标题\r\n    subtext: '', // 副标题\r\n    left: 'center'\r\n  },\r\n  tooltip: {\r\n    trigger: 'item',\r\n    formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n  },\r\n  legend: {\r\n    orient: 'vertical',\r\n    left: 'left'\r\n  },\r\n  series: [\r\n    {\r\n      name: '', // 鼠标移上去显示内容\r\n      type: 'pie',\r\n      radius: '50%',\r\n      center: ['50%', '60%'],\r\n      data: [\r\n        {value: 1048, name: '瑞幸咖啡'}, // 示例数据：name表示维度，value表示对应的值\r\n        {value: 735, name: '雀巢咖啡'},\r\n        {value: 580, name: '星巴克咖啡'},\r\n        {value: 484, name: '栖巢咖啡'},\r\n        {value: 300, name: '小武哥咖啡'}\r\n      ]\r\n    }\r\n  ]\r\n}\r\nlet lineOptions = {\r\n  title: {\r\n    text: '', // 主标题\r\n    subtext: '', // 副标题\r\n    left: 'center'\r\n  },\r\n  xAxis: {\r\n    type: 'category',\r\n    data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] // 示例数据：统计的维度（横坐标）\r\n  },\r\n  yAxis: {\r\n    type: 'value'\r\n  },\r\n  tooltip: {\r\n    trigger: 'item'\r\n  },\r\n  series: [\r\n    {\r\n      data: [120, 200, 150, 80, 70, 110, 130], // 示例数据：横坐标维度对应的值（纵坐标）\r\n      type: 'line',\r\n    }\r\n  ]\r\n}\r\nlet barOptions = {\r\n  title: {\r\n    text: '', // 主标题\r\n    subtext: '', // 副标题\r\n    left: 'center'\r\n  },\r\n  xAxis: {\r\n    type: 'category',\r\n    data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] // 示例数据：统计的维度（横坐标）\r\n  },\r\n  yAxis: {\r\n    type: 'value'\r\n  },\r\n  tooltip: {\r\n    trigger: 'item'\r\n  },\r\n  series: [\r\n    {\r\n      data: [120, 200, 150, 80, 70, 110, 130], // 示例数据：横坐标维度对应的值（纵坐标）\r\n      type: 'bar',\r\n      itemStyle: {\r\n        normal: {\r\n          color:function(){return \"#\"+Math.floor(Math.random()*(256*256*256-1)).toString(16);}\r\n        },\r\n      },\r\n    }\r\n  ]\r\n}\r\nlet barSalaryOptions = {\r\n  title: {\r\n    text: '', // 主标题\r\n    subtext: '', // 副标题\r\n    left: 'center'\r\n  },\r\n  xAxis: {\r\n    type: 'category',\r\n    data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] // 示例数据：统计的维度（横坐标）\r\n  },\r\n  yAxis: {\r\n    type: 'value'\r\n  },\r\n  tooltip: {\r\n    trigger: 'item'\r\n  },\r\n  series: [\r\n    {\r\n      data: [120, 200, 150, 80, 70, 110, 130], // 示例数据：横坐标维度对应的值（纵坐标）\r\n      type: 'bar',\r\n      itemStyle: {\r\n        normal: {\r\n          color:function(){return \"#\"+Math.floor(Math.random()*(256*256*256-1)).toString(16);}\r\n        },\r\n      },\r\n    }\r\n  ]\r\n}\r\n\r\nexport default {\r\n  name: 'Home',\r\n  components: {\r\n    WelcomeComponent\r\n  },\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      notices: [],\r\n      month: null,\r\n      monthData: [],\r\n      departmentInfo: {\r\n        name: '',\r\n        employeeCount: 0,\r\n        headerName: ''\r\n      },\r\n      pendingTasks: {\r\n        askCount: 0,\r\n        resourceCount: 0\r\n      },\r\n      personalTasks: {\r\n        total: 0,\r\n        approved: 0,\r\n        pending: 0,\r\n        rejected: 0\r\n      },\r\n      departmentEmployees: [],\r\n      mockData: {\r\n        departments: {\r\n          1: { name: '技术研发部', headerName: '刘波涛' },\r\n          2: { name: '市场营销部', headerName: '张静' },\r\n          3: { name: '人力资源部', headerName: '王飞' },\r\n          4: { name: '财务部', headerName: '刘颖' },\r\n          5: { name: '运营部', headerName: '陈伟' }\r\n        },\r\n        employeeCounts: {\r\n          1: 3, 2: 2, 3: 2, 4: 1, 5: 1\r\n        },\r\n        pendingAsks: {\r\n          1: 2, 2: 1, 3: 1, 4: 0, 5: 1\r\n        },\r\n        pendingResources: {\r\n          1: 2, 2: 1, 3: 0, 4: 1, 5: 0\r\n        },\r\n        departmentEmployees: {\r\n          1: [\r\n            { id: 1, name: '张三', phone: '13811111111', email: 'zhangsan@xm.com', level: '员工' },\r\n            { id: 2, name: '李四', phone: '13822222222', email: 'lisi@xm.com', level: '员工' },\r\n            { id: 4, name: '刘波涛', phone: '13844444444', email: 'liubotao@xm.com', level: '主管' }\r\n          ],\r\n          2: [\r\n            { id: 3, name: '王五', phone: '13833333333', email: 'wangwu@xm.com', level: '员工' },\r\n            { id: 5, name: '张静', phone: '13855555555', email: 'zhangjing@xm.com', level: '主管' }\r\n          ],\r\n          3: [\r\n            { id: 6, name: '王飞', phone: '13866666666', email: 'wangfei@xm.com', level: '主管' },\r\n            { id: 10, name: '孙莹', phone: '13800000000', email: 'sunying@xm.com', level: '员工' }\r\n          ],\r\n          4: [\r\n            { id: 7, name: '刘颖', phone: '13877777777', email: 'liuying@xm.com', level: '主管' }\r\n          ],\r\n          5: [\r\n            { id: 8, name: '陈伟', phone: '13888888888', email: 'chenwei@xm.com', level: '主管' }\r\n          ]\r\n        },\r\n        personalStats: {\r\n          total: 3,\r\n          approved: 1,\r\n          pending: 1, \r\n          rejected: 1\r\n        }\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    isHeader() {\r\n      return this.user.level === 'HEADER' || \r\n             this.user.role === 'HEADER' || \r\n             (this.user.level && this.user.level.includes('主管'));\r\n    }\r\n  },\r\n  created() {\r\n    this.$request.get('/notice/selectAll').then(res => {\r\n      this.notices = res.data || []\r\n    })\r\n    \r\n    if (this.user.role === 'ADMIN') {\r\n      this.getMonthData()\r\n      this.getFinancialPie()\r\n      this.getFinancialLine()\r\n      this.getFinancialBar()\r\n    } else if (this.isHeader) {\r\n      this.loadDepartmentInfo();\r\n      this.loadPendingTasks();\r\n      this.loadDepartmentEmployees();\r\n    } else if (this.user.role === 'USER') {\r\n      this.loadDepartmentInfo();\r\n      this.loadPersonalTasks();\r\n    }\r\n  },\r\n  methods: {\r\n    getMonthData() {\r\n      this.$request.get('/salary/getMonth').then(res => {\r\n        if (res.code === '200') {\r\n          this.monthData = res.data\r\n          this.month = this.monthData[0].year\r\n          this.getSalaryMonth(this.month)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getSalaryMonth(month) {\r\n      this.$request.get('/salary/getSalary/' + month).then(res => {\r\n        if (res.code === '200') {\r\n          let chartDom = document.getElementById('salaryBar');\r\n          let myChart = echarts.init(chartDom);\r\n          barSalaryOptions.title.text = res.data.text\r\n          barSalaryOptions.title.subtext = res.data.subtext\r\n          barSalaryOptions.xAxis.data = res.data.xAxis\r\n          barSalaryOptions.series[0].data = res.data.yAxis\r\n          myChart.setOption(barSalaryOptions)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getFinancialPie() {\r\n      this.$request.get('/financial/getPie').then(res => {\r\n        if (res.code === '200') {\r\n          let chartDom = document.getElementById('pie');\r\n          let myChart = echarts.init(chartDom);\r\n          pieOptions.title.text = res.data.text\r\n          pieOptions.title.subtext = res.data.subtext\r\n          pieOptions.series[0].name = res.data.name\r\n          pieOptions.series[0].data = res.data.data\r\n          myChart.setOption(pieOptions);\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getFinancialLine() {\r\n      this.$request.get('/financial/getLine').then(res => {\r\n        if (res.code === '200') {\r\n          let chartDom = document.getElementById('line');\r\n          let myChart = echarts.init(chartDom);\r\n          lineOptions.title.text = res.data.text\r\n          lineOptions.title.subtext = res.data.subtext\r\n          lineOptions.xAxis.data = res.data.xAxis\r\n          lineOptions.series[0].data = res.data.yAxis\r\n          myChart.setOption(lineOptions)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getFinancialBar() {\r\n      this.$request.get('/financial/getBar').then(res => {\r\n        if (res.code === '200') {\r\n          let chartDom = document.getElementById('bar');\r\n          let myChart = echarts.init(chartDom);\r\n          barOptions.title.text = res.data.text\r\n          barOptions.title.subtext = res.data.subtext\r\n          barOptions.xAxis.data = res.data.xAxis\r\n          barOptions.series[0].data = res.data.yAxis\r\n          myChart.setOption(barOptions)\r\n        }\r\n      })\r\n    },\r\n    loadDepartmentInfo() {\r\n      if (this.user.departmentId) {\r\n        this.$request.get('/department/findById/' + this.user.departmentId).then(res => {\r\n          if (res.code === '200') {\r\n            this.departmentInfo.name = res.data.name;\r\n            this.departmentInfo.headerName = res.data.headerName || '未指定';\r\n            \r\n            if (this.isHeader) {\r\n              this.loadEmployeeCount();\r\n            }\r\n          }\r\n        }).catch(error => {\r\n          console.error(\"获取部门信息失败:\", error);\r\n          const deptId = this.user.departmentId;\r\n          if (this.mockData.departments[deptId]) {\r\n            console.log(\"使用模拟部门数据\");\r\n            this.departmentInfo.name = this.mockData.departments[deptId].name;\r\n            this.departmentInfo.headerName = this.mockData.departments[deptId].headerName;\r\n            \r\n            if (this.isHeader) {\r\n              this.loadEmployeeCount();\r\n            }\r\n          } else {\r\n            this.$message.error(\"获取部门信息失败，请检查网络连接\");\r\n          }\r\n        });\r\n      }\r\n    },\r\n    loadEmployeeCount() {\r\n      this.$request.get('/employee/count', {\r\n        params: { departmentId: this.user.departmentId }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.departmentInfo.employeeCount = res.data || 0;\r\n        }\r\n      }).catch(error => {\r\n        console.error(\"获取员工数量失败:\", error);\r\n        const deptId = this.user.